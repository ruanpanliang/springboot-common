<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lc.springboot.user.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lc.springboot.user.model.Dict">
        <result column="ID" property="id" />
        <result column="CREATED_BY" property="createdBy" />
        <result column="CREATED_TIME" property="createdTime" />
        <result column="UPDATED_BY" property="updatedBy" />
        <result column="UPDATED_TIME" property="updatedTime" />
        <result column="RANDOM_CODE" property="randomCode" />
        <result column="REVISION" property="revision" />
        <result column="DELETE_FLAG" property="deleteFlag" />
        <result column="DICT_CODE" property="dictCode" />
        <result column="DICT_NAME" property="dictName" />
        <result column="DICT_TYPE_CODE" property="dictTypeCode" />
        <result column="STATUS" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID,
        CREATED_BY,
        CREATED_TIME,
        UPDATED_BY,
        UPDATED_TIME,
        RANDOM_CODE,
        REVISION,
        DELETE_FLAG,
        DICT_CODE, DICT_NAME, DICT_TYPE_CODE, STATUS
    </sql>

</mapper>
